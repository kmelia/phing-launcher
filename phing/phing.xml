<?xml version="1.0" encoding="UTF-8"?>
<project>
    <target name="phing.run-command" hidden="true">
        <if>
            <not>
                <isset property="phing.command-level" />
            </not>
            <then>
                <property name="phing.command-level" value="info" />
            </then>
        </if>
        
        <exec command="${phing.command-line}" logoutput="true" checkreturn="true" level="${phing.command-level}" />
    </target>
</project>
